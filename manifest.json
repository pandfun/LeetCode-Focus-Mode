{
    "manifest_version": 3,
    "name": "LeetCode Focus Mode",
    "version": "1.0",
    "description": "Hide difficulty and acceptance rate on LeetCode.",
    "permissions": ["scripting"],
    "content_scripts": [
      {
        "matches": ["*://leetcode.com/*"],
        "js": ["content.js"]
      }
    ]
}
  